		Person.Adr.House = prompt("Введите Дом:","");
		Person.Adr.Office = prompt("Введите Офис:","");
		
		//
		
		
		for (var val in Person){
			if (typeof(Person[val]) == "object"){
				for (var inval in Person[val]){
					document.write("  -" + Person[val][inval] + "<br/>");
				}
			}
			else {
